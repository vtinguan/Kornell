<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

  <g:FlowPanel addStyleNames="deanWrapper">
		<g:HTMLPanel addStyleNames="title">Edit Repositories</g:HTMLPanel>
		<g:Label addStyleNames="subTitle">Configure a repository where your institution data is stored</g:Label>
		<b:Form ui:field="form" addStyleNames="profileForm">
			<g:FlowPanel ui:field="generalFieldsPanel" addStyleNames="fieldsWrapper marginTop25">		
			</g:FlowPanel>
			<g:FlowPanel ui:field="s3FieldsPanel" addStyleNames="fieldsWrapper marginTop25">		
			</g:FlowPanel>
			<g:FlowPanel ui:field="fsFieldsPanel" addStyleNames="fieldsWrapper marginTop25">		
			</g:FlowPanel>	
			<g:FlowPanel addStyleNames="standardButtonBar">
				<g:Button ui:field="btnOK" addStyleNames="btnAction btnStandard"/>
				<g:Button ui:field="btnCancel" addStyleNames="btnNotSelected btnStandard"/>
			</g:FlowPanel>
		</b:Form>		
	</g:FlowPanel>

</ui:UiBinder>