<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

  <g:FlowPanel addStyleNames="deanWrapper">
		<g:HTMLPanel addStyleNames="title">Editar Imagens da Instituição</g:HTMLPanel>
		<g:Label addStyleNames="subTitle">Configure logos da sua instituição</g:Label>
		<b:Form ui:field="form" addStyleNames="profileForm">
			<g:FlowPanel ui:field="assetsFields" addStyleNames="fieldsWrapper marginTop25">		
			</g:FlowPanel>		
			<g:FlowPanel addStyleNames="standardButtonBar shy">
				<g:Button ui:field="btnOK" addStyleNames="btnAction btnStandard"/>
				<g:Button ui:field="btnCancel" addStyleNames="btnNotSelected btnStandard"/>
			</g:FlowPanel>
		</b:Form>		
	</g:FlowPanel>

</ui:UiBinder>