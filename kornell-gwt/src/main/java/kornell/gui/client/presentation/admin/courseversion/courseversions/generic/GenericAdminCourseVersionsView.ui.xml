<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys='com.google.gwt.i18n.server.keygen.MD5KeyGenerator'
    ui:generateLocales='default'>

	<g:FlowPanel ui:field="adminHomePanel" addStyleNames="deanHome">
	    <g:FlowPanel ui:field="courseVersionsPanel" addStyleNames="courseClassesHeaderWrapper">
		    <g:FlowPanel addStyleNames="left">
		      <g:HTMLPanel addStyleNames="title"><ui:msg description="Course version panel title">Gerenciar Versões de Curso</ui:msg></g:HTMLPanel>
		      <g:HTMLPanel addStyleNames="subTitle"><ui:msg description="Course version panel sub-title">Escolha abaixo a versão de curso que deseja gerenciar</ui:msg></g:HTMLPanel>
		    </g:FlowPanel>
			<b:Button ui:field="btnAddCourseVersion" addStyleNames="btnAction btnIcon right" size="MINI" icon="PLUS_SIGN"/>
		</g:FlowPanel>
	    <g:FlowPanel ui:field="courseVersionsWrapper" addStyleNames="courseClassesHeaderWrapper">
		</g:FlowPanel>
		<g:FlowPanel ui:field="createVersionPanel">
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>